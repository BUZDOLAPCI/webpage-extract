{
  "name": "webpage-extract",
  "version": "1.0.0",
  "description": "MCP server for fetching webpages and extracting readable Markdown, tables, and metadata",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev:stdio": "tsx src/index.ts --transport stdio",
    "dev:http": "tsx src/index.ts --transport http",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "webpage",
    "html",
    "markdown",
    "extraction"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "cheerio": "^1.0.0",
    "turndown": "^7.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/turndown": "^5.0.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
